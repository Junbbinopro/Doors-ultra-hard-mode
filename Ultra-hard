-- Sprint

local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

--// Settings
local WALK_SPEED = 16
local SPRINT_SPEED = 20
local MAX_STAMINA = 100
local stamina = MAX_STAMINA
local drainRate = 15 -- Points per second
local recoverRate = 10 -- Points per second
local isSprinting = false

--// Create UI
local screenGui = Instance.new("ScreenGui", player.PlayerGui)
screenGui.Name = "SprintSystem"
screenGui.ResetOnSpawn = false

-- Stamina Background
local barBack = Instance.new("Frame", screenGui)
barBack.Name = "StaminaBack"
barBack.Size = UDim2.new(0, 200, 0, 10)
barBack.Position = UDim2.new(0.5, -100, 0.9, 0)
barBack.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
barBack.BorderSizePixel = 0

-- Actual Bar
local barFill = Instance.new("Frame", barBack)
barFill.Name = "Fill"
barFill.Size = UDim2.new(1, 0, 1, 0)
barFill.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White like Doors
barFill.BorderSizePixel = 0

-- Mobile Button
local mobileBtn = Instance.new("TextButton", screenGui)
mobileBtn.Name = "SprintBtn"
mobileBtn.Size = UDim2.new(0, 65, 0, 65)
mobileBtn.Position = UDim2.new(0.75, 0, 0.78, 0)
mobileBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mobileBtn.BackgroundTransparency = 0.4
mobileBtn.Text = "RUN"
mobileBtn.TextColor3 = Color3.new(1, 1, 1)
mobileBtn.Font = Enum.Font.GothamBold
mobileBtn.TextSize = 14
Instance.new("UICorner", mobileBtn).CornerRadius = UDim.new(1, 0)

--// Logic
local function updateSprint()
	if isSprinting and humanoid.MoveDirection.Magnitude > 0 and stamina > 0 then
		humanoid.WalkSpeed = SPRINT_SPEED
		stamina = math.max(0, stamina - (drainRate * RunService.RenderStepped:Wait()))
	else
		humanoid.WalkSpeed = WALK_SPEED
		stamina = math.min(MAX_STAMINA, stamina + (recoverRate * RunService.RenderStepped:Wait()))
		if stamina <= 0 then isSprinting = false end -- Force stop if empty
	end
	
	-- Update UI
	barFill.Size = UDim2.new(stamina / MAX_STAMINA, 0, 1, 0)
	barFill.BackgroundTransparency = (stamina >= MAX_STAMINA) and 1 or 0
	barBack.BackgroundTransparency = (stamina >= MAX_STAMINA) and 1 or 0
	mobileBtn.BackgroundColor3 = isSprinting and Color3.fromRGB(200, 0, 0) or Color3.fromRGB(0, 0, 0)
end

-- Input Handlers
mobileBtn.MouseButton1Click:Connect(function()
	if stamina > 10 then isSprinting = not isSprinting end
end)

UIS.InputBegan:Connect(function(input, gpe)
	if not gpe and input.KeyCode == Enum.KeyCode.LeftShift then isSprinting = true end
end)

UIS.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift then isSprinting = false end
end)

RunService.RenderStepped:Connect(updateSprint)
   
-- Guiding Candle

loadstring(game:HttpGet("https://raw.githubusercontent.com/Junbbinopro/Guiding-light-candle/refs/heads/main/Candle"))()
